#include <stdio.h>

int main() {
    int a, b, c, x;
    
    printf("Enter three numbers: ");
    scanf("%d %d %d", &a, &b, &c);

    if ((a > b && a < c) || (a < b && a > c)) {
        x = a; 
    } 
    else if ((b > a && b < c) || (b < a && b > c)) {
        x = b;  
    } 
    else {
        x = c;
    }

    printf("The middle number is: %d\n", x);
    return 0;
}
